<% layout('./layouts/boilerplate') %>

<h1>All Campgrounds</h1>
<a href="/campgrounds/new">Add a campground</a>
<ul>
    <% for (let campground of campgrounds) { %>
    <li>
        <a href="/campgrounds/<%= campground._id %> "
            ><%= campground.title %></a
        >
        - <b><%= campground.location %> </b>
    </li>
    <% } %>
</ul>
